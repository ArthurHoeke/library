{
  "name": "polkadot-cloud-assets",
  "license": "GPL-3.0-only",
  "version": "0.3.0",
  "type": "module",
  "contributors": [
    "Ross Bulat<ross@parity.io>",
    "Nikolaos Kontakis<nikos@parity.io>"
  ],
  "description": "Data sources and static assets for Polkadot dApps.",
  "keywords": [
    "Polkadot",
    "Validators",
    "Extensions",
    "Web3",
    "Components",
    "UI"
  ],
  "bugs": {
    "url": "https://github.com/polkadot-cloud/library/issues"
  },
  "homepage": "https://github.com/polkadot-cloud/library#readme",
  "scripts": {
    "prebuild": "node ./lib/external/polkadot_directory_repo.js",
    "build:mock": "node ../../scripts/generatePackageJson.mjs -p assets",
    "build": "rm -rf dist && gulp && cp LICENSE dist && cp README.npm.md dist && mv dist/README.npm.md dist/README.md && node ../../scripts/generatePackageJson.mjs -p assets",
    "postbuild": "rm -rf ./lib/external/yaml ./lib/external/json ./lib/external/index.ts",
    "clear": "rm -rf node_modules dist tsconfig.tsbuildinfo"
  }
}
